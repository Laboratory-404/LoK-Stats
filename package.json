{
	"name": "lok",
	"private": true,
	"license": "UNLICENSED",
	"version": "0.0.1",
	"description": "League of Kingdoms",
	"author": "ZDS",
	"type": "module",
	"engines": {
		"node": "^16.14.1",
		"npm": "^8.5.0"
	},
	"scripts": {
		"preinstall": "npx --yes force-resolutions",
		"clean": "run-s clean:kit clean:app",
		"clean:kit": "rimraf ./.svelte-kit",
		"clean:app": "rimraf ./docs",
		"build": "svelte-kit build && mklink docs\\404.html docs\\index.html",
		"dev": "svelte-kit dev --port 4000",
		"preview": "svelte-kit preview",
		"package": "svelte-kit package",
		"prepare": "svelte-kit sync",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@poppanator/sveltekit-svg": "^0.3.1",
		"@sveltejs/adapter-static": "1.0.0-next.29",
		"@sveltejs/kit": "1.0.0-next.301",
		"@types/cookie": "^0.4.1",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"autoprefixer": "^10.4.4",
		"cssnano": "^5.1.5",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.12",
		"postcss-load-config": "^3.1.3",
		"postcss-svgo": "^5.1.0",
		"prettier": "^2.6.1",
		"prettier-plugin-svelte": "^2.6.0",
		"replace-in-files-cli": "^2.0.0",
		"rimraf": "^3.0.2",
		"sass": "^1.49.9",
		"svelte": "^3.46.4",
		"svelte-check": "^2.4.6",
		"svelte-preprocess": "^4.10.4",
		"svelte-simple-datatables": "^0.2.3",
		"tslib": "^2.3.1",
		"typescript": "~4.6.3"
	},
	"dependencies": {
		"@lukeed/uuid": "^2.0.0",
		"@popperjs/core": "^2.11.4",
		"@tailwindcss/forms": "^0.5.0",
		"@tailwindcss/typography": "^0.5.2",
		"bootstrap": "^5.1.3",
		"browser-cookies": "^1.2.0",
		"cookie": "^0.4.2",
		"dexie": "^3.2.1",
		"idb-keyval": "^6.1.0",
		"saos": "^1.2.0",
		"svelte-feather-icons": "^3.5.1",
		"tailwindcss": "^3.0.23"
	},
	"resolutions": {
		"@sveltejs/vite-plugin-svelte": "1.0.0-next.40"
	}
}